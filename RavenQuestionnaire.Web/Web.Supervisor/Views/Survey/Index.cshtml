@using RavenQuestionnaire.Core.Views.Survey
@model SurveyBrowseView
@{
    ViewBag.Title = "Surveys";
}
<h2>Surveys</h2>
<table class="table table-striped table-bordered table-condensed">
<thead>
    <tr>
        <th>Survey</th>
        <th>Initial date</th>
        @foreach (var item in Model.Headers)
        {
            <th>@item</th>
        }
    </tr>
</thead>
@foreach (var collect in Model.Items)
{
    <tr>
        <td>@Html.ActionLink(collect.Title, "Assigments", new { id=collect.Id })</td>        
        <td></td>
            @foreach (var it in collect.Grid)
            {
                <td>@it.Value</td>
            }
    </tr>
}
</table>
