@using Main.Core.View.Group
@model Main.Core.View.Group.CompleteGroupMobileView

<ul class="nav nav-list bs-docs-sidenav" id="propagate-list-@(Model.PublicKey)">
    @for (int i = 0; i < Model.Children.Count; i++)
    {
        ViewBag.GroupIndex = i + 1;
        var propagatedGroup = Model.Children[i] as PropagatedGroupMobileView;
        @Html.Partial("_PropagatedGroup.cshtml", propagatedGroup)
    }
</ul>