@using Core.Supervisor.Views.SyncProcess
@model SyncProcessLogView
@{
    ViewBag.Title = "Index";
}
<h3>Syncronization logs</h3>
<table  class="table table-striped table-bordered table-condensed table-hover">
    <thead>
        <tr>
            <td>
                Public Key
            </td>
            <td>
                Parent Key
            </td>
            <td>
                Start Date
            </td>
            <td>
                End Date
            </td>
            <td>
                Sync Type
            </td>
            <td>
                Description
            </td>
            <td>
                Exit Description
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Items)
        {
            <tr>
                <td>@item.PubliKey
                </td>
                <td>@item.ParentKey
                </td>
                <td>@item.StartDate.ToLocalTime()
                </td>
                <td>@item.EndDate.ToLocalTime()
                </td>
                <td>@item.SyncType
                </td>
                <td>@item.Description
                </td>
                <td>@item.ExitDescription
                </td>
            </tr>
        }
    </tbody>
</table>
