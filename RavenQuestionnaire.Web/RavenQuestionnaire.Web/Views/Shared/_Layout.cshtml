<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/icons.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.theme.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.datepicker.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.numeric.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script>
</head>
<body>
    <div class="topbar">
        <div class="topbar-inner">
            <div class="container-fluid">
                <a href="@Url.Action("MyItems", "CompleteQuestionnaire")" class="brand">Home</a>
                 <ul class="nav">@*
                    <li>@Html.ActionLink("Dashboard", "MyItems", "CompleteQuestionnaire")</li>*@
                    <li>@Html.ActionLink("Take New", "ItemList", "Questionnaire")</li>
                    <li>@Html.ActionLink("All Completed", "Index", "CompleteQuestionnaire")</li>
                </ul>
                <p class="pull-right">@Html.Partial("_LogOnPartial")</p>
            </div>
        </div>
    </div>
    <div class="container-fluid">
        <div id="main">
            @RenderBody()
        </div>
    </div>
</body>
</html>