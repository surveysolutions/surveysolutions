@model Main.Core.View.Questionnaire.QuestionnaireBrowseView
@foreach (var item in Model.Items)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreationDate)
        </td>
       @* <td>
            @Html.DisplayFor(modelItem => item.LastEntryDate)
        </td>*@
        <td class="buttons">
            @Html.ActionLink("Flow", "Edit", "FlowGraph", new { id = item.Id }, new { @class = "btn small " })
        <td class="buttons">    
            @Html.ActionLink("Manage", "Details", new { id = item.Id }, new { @class = "btn small " })
        </td>
        <td class="buttons">    
            @Html.ActionLink("Export", "Export", new { id = item.Id }, new { @class = "btn small btn-primary" })
        </td>
        <td class="buttons">    
            @Html.ActionLink("Download", "Download", "ImportExport", new { id = item.Id, clientGuid = "00000000-0000-0000-0000-000000000000" }, new { @class = "btn small btn-primary" })
        </td>
    </tr>
}