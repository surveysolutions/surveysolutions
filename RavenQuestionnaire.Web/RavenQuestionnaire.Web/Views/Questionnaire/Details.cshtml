<div id="busyindicator"></div>
<div id="stacks">
    <div id="groups" class="stack">
        <div class="inner">
            <div class="title">
                <button type="button" class="close" data-bind="click: closeMenu">&times</button>
            </div>
            <div class="body">
            </div>
            <div class="bottom">
                <div class="inner">
                    <button class="btn btn-success"><i class="icon-plus icon-white"></i> Chapter</button>
                </div>
            </div>
        </div>
    </div>
    <div id="answers" class="stack">
        <div class="inner">
            <div class="body">
            </div>
            <div class="bottom">
                <div class="inner">
                </div>
            </div>
        </div>
    </div>
    <div id="details" class="stack">
        <div class="inner">
            <div class="title">
                <button type="button" class="close">&times</button>
            </div>
            <div class="body">
            </div>
            <div class="bottom">
                <div class="inner">
                </div>
            </div>
        </div>
    </div>
</div>