@using RavenQuestionnaire.Core.Views
@using RavenQuestionnaire.Core.Views.Group
@using RavenQuestionnaire.Core.Views.Question

@model IEnumerable<ICompositeView>
           
@foreach (var item in Model)
{
    if ((item as QuestionView)!=null)
    {
        Html.RenderPartial("~/Views/Question/_QuestionRow.cshtml", item as QuestionView);
    }
    else
    {
        Html.RenderPartial("~/Views/Group/_GroupRow.cshtml", item as GroupView);
    }
}