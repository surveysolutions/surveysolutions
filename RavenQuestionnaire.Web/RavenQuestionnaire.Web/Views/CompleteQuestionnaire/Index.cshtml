@using RavenQuestionnaire.Core.Views.CompleteQuestionnaire
@model CompleteQuestionnaireBrowseView

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table>
    <tr>
        <th>
            Id
        </th>
        <th>
            Title
        </th>
        <th>Creation date</th>
        <th>Last edit date</th>
        <th></th>
    </tr>

@foreach (var item in Model.Items) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QuestionnaireTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreationDate)
        </td>
          <td>
            @Html.DisplayFor(modelItem => item.LastEntryDate)
        </td>
        <td>
            @Html.ActionLink("Details", "Result", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new {  id=item.Id })
        </td>
    </tr>
}

</table>
<div class="pager">
    @Html.Pager(Model.PageSize, Model.Page, Model.TotalCount)
</div>
