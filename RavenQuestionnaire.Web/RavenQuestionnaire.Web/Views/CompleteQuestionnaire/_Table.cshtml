@using RavenQuestionnaire.Core.Views.CompleteQuestionnaire
@model CompleteQuestionnaireBrowseView
@foreach (var item in Model.Items)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CompleteQuestionnaireId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QuestionnaireTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastEntryDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status.Name)
            @Html.HiddenFor(modelItem => item.Status.PublicId)
        </td>
        <td class="buttons">
            @Html.ActionLink("Details", "Result", new { id = item.CompleteQuestionnaireId }, new { @class = "btn" })

            @Html.ActionLink("Edit", "QuestionHtml5", "CompleteQuestionnaire", new { id = item.CompleteQuestionnaireId }, new { @class = "btn" })
        </td>
    </tr>
}