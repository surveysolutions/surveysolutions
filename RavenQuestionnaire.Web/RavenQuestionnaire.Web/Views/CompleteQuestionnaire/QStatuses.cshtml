@using RavenQuestionnaire.Core.Views.StatusReport
@model CQStatusReportView
@{
    ViewBag.Title = "QStatuses";
}
<h2>@Model.StatusName</h2>
<table class="table table-striped table-bordered table-condensed">
    <thead>
        <tr>
            <th>Description</th>
            <th>Assign to</th>
            <th>Last sync</th>
            <th>Last change date</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Items)
        {
            <tr>
                <td>
                    @item.Description
                </td>
                <td>
                    @item.AssignToUser.Name
                </td>
                <td>
                    @item.LastSyncDate
                </td>
                <td>
                    @item.LastChangeDate
                </td>
                <td>
                </td>
            </tr>
        }
    </tbody>
</table>
