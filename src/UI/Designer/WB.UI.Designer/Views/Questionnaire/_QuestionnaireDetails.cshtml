<div class="title">
    <button type="button" class="close" data-bind="click: $root.closeDetails">&times</button>
    <span data-bind="text: title"></span>
</div>
<div class="body container-fluid">
    <div class="row-fluid">
        <div class="control-group">
            <label class="control-label required">Title</label>
            <div class="controls">
                <input type="text" data-bind="value: title, disable: true" class="span12" />
            </div>
        </div>
    </div>
    <div class="row-fluid">
        <div class="control-group">
            <div class="controls">
                <label class="checkbox">
                    <input type="checkbox" data-bind="checked: isPublic, disable: true" />Is public?
                </label>
            </div>
        </div>
    </div>
</div>
<div class="pane-bottom">
</div>
