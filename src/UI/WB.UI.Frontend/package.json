{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env HOT_MODE=1 vue-cli-service serve",
    "build": "vue-cli-service build",
    "styles": "vue-cli-service build --css",
    "styles:dev": "vue-cli-service build --css --mode development",
    "dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "hot": "vue-cli-service build --mode development --watch",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@google/markerclustererplus": "^2.1.11",
    "@microsoft/signalr": "^3.1.0",
    "ag-grid-community": "^21.2.1",
    "ag-grid-vue": "^21.2.1",
    "append-query": "^2.1.0",
    "autonumeric": "^4.5.3",
    "axios": "0.19.0",
    "bootbox": "^4.4.0",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.4.1",
    "bootstrap-select": "1.12.4",
    "chart.js": "^2.7.3",
    "core-js": "^3.5.0",
    "datatables.net": "^1.10.19",
    "datatables.net-select": "1.2.5",
    "filemanager-webpack-plugin": "^2.0.2",
    "flatpickr": "^4.6.2",
    "fuse.js": "^3.2.0",
    "i18next": "^17.0.14",
    "jquery": "^3.4.1",
    "jquery-contextmenu": "^2.6.3",
    "jquery-highlight": "^3.4.0",
    "jquery-mask-plugin": "^1.14.16",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "pnotify": "^3.2.1",
    "popper.js": "^1.16.0",
    "smoothscroll-polyfill": "^0.4.0",
    "sticky-kit": "^1.1.3",
    "toastr": "^2.1.4",
    "uiv": "^0.32.2",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-infinite-loading": "^2.4.3",
    "vue-router": "^3.1.3",
    "vue-textarea-autosize": "^1.1.1",
    "vue-timers": "^2.0.4",
    "vue2-editor": "^2.10.2",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.0.1",
    "extra-watch-webpack-plugin": "^1.0.3",
    "node-sass": "^4.13.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "stats-webpack-plugin": "^0.7.0",
    "vue-property-decorator": "^8.2.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "write-file-webpack-plugin": "^4.5.1",
    "xmldoc": "^1.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie > 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "testPathIgnorePatterns": [
      "/locale//.resources/"
    ]
  }
}
