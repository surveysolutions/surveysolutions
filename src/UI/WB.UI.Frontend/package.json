{
  "name": "hello-world",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env HOT_MODE=1 vue-cli-service serve",
    "build": "vue-cli-service build",
    "dev": "vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "hot": "gulp hot",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@google/markerclustererplus": "^2.1.11",
    "@microsoft/signalr": "^3.0.0",
    "ag-grid-community": "^21.2.1",
    "ag-grid-vue": "^21.2.1",
    "append-query": "^2.1.0",
    "autonumeric": "^4.5.3",
    "axios": "0.19.0",
    "bootbox": "^4.4.0",
    "bootstrap": "^3.3.7",
    "bootstrap-select": "1.12.4",
    "chart.js": "^2.7.3",
    "datatables.net": "^1.10.19",
    "datatables.net-select": "1.2.5",
    "es6-promise": "^4.2.8",
    "filemanager-webpack-plugin": "^2.0.2",
    "flatpickr": "^4.6.2",
    "fuse.js": "^3.2.0",
    "i18next": "^17.0.14",
    "jquery": "^3.2.1",
    "jquery-contextmenu": "^2.6.3",
    "jquery-highlight": "^3.4.0",
    "jquery-mask-plugin": "^1.14.16",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "pnotify": "^3.2.1",
    "scriptjs": "^2.5.9",
    "signalr": "^2.2.2",
    "smoothscroll-polyfill": "^0.4.0",
    "sticky-kit": "^1.1.3",
    "toastr": "^2.1.4",
    "uiv": "^0.32.2",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-infinite-loading": "^2.4.3",
    "vue-router": "^3.1.3",
    "vue-textarea-autosize": "^1.1.1",
    "vue-timers": "^2.0.4",
    "vue2-editor": "^2.10.2",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "cross-env": "^5.2.0",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-util": "^3.0.8",
    "rimraf": "^3.0.0",
    "stats-webpack-plugin": "^0.7.0",
    "vue-property-decorator": "^8.2.2",
    "vue-template-compiler": "^2.6.10",
    "write-file-webpack-plugin": "^4.5.1",
    "xmldoc": "^1.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie > 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "testPathIgnorePatterns": [
      "/locale//.resources/"
    ]
  }
}
