{
    "name": "hello-world",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "cross-env HOT_MODE=1 vue-cli-service serve",
        "build": "vue-cli-service build",
        "test:unit": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint --fix --format stylish",
        "dev": "vue-cli-service build --mode development",
        "hot": "vue-cli-service build --mode development --watch",
        "styles": "vue-cli-service build --css",
        "styles:dev": "vue-cli-service build --css --mode development"
    },
    "dependencies": {
        "@google/markerclustererplus": "^4.0.1",
        "@microsoft/signalr": "^5.0.6",
        "@toast-ui/vue-editor": "^2.5.2",
        "ag-grid-community": "^25.3.0",
        "ag-grid-vue": "^25.3.0",
        "append-query": "^2.1.1",
        "autonumeric": "^4.5.3",
        "axios": "^0.21.1",
        "bootbox": "^5.4.0",
        "bootstrap": "^3.3.7",
        "bootstrap-sass": "^3.4.1",
        "bootstrap-select": "^1.13.15",
        "chart.js": "^2.9.4",
        "core-js": "^3.13.1",
        "datatables.net": "^1.10.24",
        "datatables.net-select": "^1.3.3",
        "filemanager-webpack-plugin": "^2.0.2",
        "flatpickr": "^4.6.9",
        "i18next": "^20.3.1",
        "isomorphic-unfetch": "^3.0.0",
        "jquery": "^3.6.0",
        "jquery-contextmenu": "^2.9.2",
        "jquery-highlight": "^3.4.0",
        "jquery-mask-plugin": "^1.14.16",
        "lodash": "^4.17.21",
        "marked": "^2.0.7",
        "moment": "^2.24.0",
        "moment-duration-format": "^2.3.2",
        "moment-timezone": "^0.5.33",
        "pnotify": "^3.2.1",
        "popper.js": "^1.16.0",
        "qrcode": "^1.4.4",
        "sanitize-html": "^2.4.0",
        "smoothscroll-polyfill": "^0.4.0",
        "sticky-kit": "^1.1.3",
        "toastr": "^2.1.4",
        "uiv": "^0.34.2",
        "vee-validate": "^2.2.15",
        "vue": "^2.6.13",
        "vue-apollo": "^3.0.7",
        "vue-chartjs": "^3.4.2",
        "vue-datatables-net": "^1.5.2",
        "vue-infinite-loading": "^2.4.3",
        "vue-page-title": "^1.2.4",
        "vue-query-builder": "^0.8.2",
        "vue-recaptcha": "^1.3.0",
        "vue-router": "^3.5.1",
        "vue-textarea-autosize": "^1.1.1",
        "vue2-editor": "^2.10.2",
        "vuex": "^3.6.2",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.5.13",
        "@vue/cli-plugin-eslint": "^4.5.13",
        "@vue/cli-service": "^4.5.13",
        "babel-eslint": "^10.0.3",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.8.0",
        "eslint-plugin-graphql": "^3.1.1",
        "eslint-plugin-vue": "^6.1.2",
        "extra-watch-webpack-plugin": "^1.0.3",
        "graphql-tag": "^2.12.4",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "5.0.0",
        "rimraf": "3.0.2",
        "sass-loader": "10.1.0",
        "stats-webpack-plugin": "^0.7.0",
        "vue-cli-plugin-apollo": "~0.22.2",
        "vue-property-decorator": "^8.2.2",
        "vue-template-compiler": "^2.6.13",
        "webpack-build-notifier": "^2.2.1",
        "webpack-livereload-plugin": "^2.3.0",
        "write-file-webpack-plugin": "^4.5.1",
        "xmldoc": "^1.1.2"
    },
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "vueIndentScriptAndStyle": false
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "last 2 versions",
        "ie > 10"
    ],
    "jest": {
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "vue"
        ],
        "transform": {
            "^.+\\.vue$": "vue-jest",
            ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
            "^.+\\.jsx?$": "babel-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "snapshotSerializers": [
            "jest-serializer-vue"
        ],
        "testMatch": [
            "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
        ],
        "testURL": "http://localhost/",
        "testPathIgnorePatterns": [
            "/locale//.resources/"
        ]
    },
    "resolutions": {
        "jquery": "3.4.1"
    }
}
