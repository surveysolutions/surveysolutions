        "jquery-highlight": "^3.4.0",
        "jquery-mask-plugin": "^1.14.16",
        "lodash": "^4.17.21",
        "marked": "^15.0.6",
        "mitt": "^3.0.1",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.46",
        "portal-vue": "^3.0.0",
        "qrcode": "^1.5.4",
        "query-builder-vue-3": "^1.0.1",
        "sanitize-html": "2.14.0",
        "smoothscroll-polyfill": "^0.4.0",
        "toastr": "^2.1.4",
        "uiv": "^2.0.6",
        "v3-infinite-loading": "^1.3.2",
        "vee-validate": "^4.15.0",
        "vue": "3.5.13",
        "vue-chartjs": "^5.3.1",
        "vue-dompurify-html": "^5.2.0",
        "vue-page-title": "^2.2.2",
        "vue-recaptcha": "^2.0.3",
        "vue-router": "^4.4.5",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "5.2.4",
        "@vue/test-utils": "^2.4.6",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.27.0",
        "fast-glob": "^3.3.3",
        "filemanager-plugin": "2.8.2",
        "jest": "^29.7.0",
        "npm-force-resolutions": "0.0.6",
        "prettier": "3.0.0",
        "rimraf": "^6.0.1",
        "rollup": "4.32.0",
        "sass": "^1.85.0",
        "terser": "^5.37.0",
        "vite": "6.4.0",
        "xmldoc": "^1.1.2"
    },
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "vueIndentScriptAndStyle": false
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "last 2 versions",
        "ie > 10"
    ],
    "jest": {
        "verbose": true,
        "testEnvironment": "jsdom",
        "setupFiles": [
            "./tests/setup-jest.js"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "vue"
        ],
        "transform": {
            "^.+\\.vue$": "vue-jest",
            ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
            "^.+\\.jsx?$": "babel-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1",
            "^~/(.*)$": "<rootDir>/src/$1"
        },
        "snapshotSerializers": [
            "jest-serializer-vue"
        ],
        "testMatch": [
            "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)",
            "**/__tests__/**/*.(test|spec).(js|jsx|ts|tsx)"
        ],
        "testURL": "http://localhost/",
        "testPathIgnorePatterns": [
            "/locale//.resources/"
        ]
    },
    "resolutions": {
        "@apollo/client": "3.7.9",
        "@babel/traverse": ">=7.23.3",
        "@types/ejs": "3.1.8",
        "async": ">=3.2.3",
        "braces": ">=2.3.1",
        "cross-fetch": "3.1.5",
        "core-js": "^3.28.0",
        "dicer": ">=0.3.1",
        "dompurify": ">=3.2.4",
        "ejs": "3.1.10",
        "follow-redirects": ">=1.15.6",
        "git-clone": "0.2.0",
        "git-parse": ">=1.0.5",
        "git-url-parse": "13.1.0",
        "graphql": "16.8.1",
        "graphql-tools": "8.3.15",
        "graphql-ws": "5.14.0",
        "glob": ">=11.0.1",
        "glob-parent": "6.0.2",
        "jest-config": "29.0.3",
        "jest-environment-jsdom": "29.0.3",
        "json-schema": ">=0.4.0",
        "json5": "2.2.3",
        "yargs-parser": ">=13.1.2",
        "merge": ">=2.1.1",
        "moment": "2.29.4",
        "loader-utils": "2.0.4",
        "nanoid": ">=2.1.11",
        "node-fetch": "3.2.10",
        "node-forge": "1.3.1",
        "node-notifier": ">=8.0.1",
        "normalize-url": ">=4.5.1",
        "nth-check": "2.1.1",
        "postcss": ">=8.4.35",
        "parse-url": "8.1.0",
        "rollup": "4.28.0",
        "sanitize-html": ">=2.13.1",
        "selfsigned": ">=1.3.0",
        "shelljs": ">=0.8.5",
        "svgo": ">=2.8.0",
        "trim-newlines": ">=3.0.1",
        "ts-node": "10.8.0",
        "vue-jest": ">=3.0.7",
        "ua-parser-js": "1.0.33",
        "ws": ">=8.18.0"
    }
}