    "type": "module",
    "private": true,
    "scripts": {
        "serve": "vite preview",
        "build": "vite build",
        "dev": "vite build --mode development",
        "test": "jest",
        "hot": "vite",
        "preinstall": "npx npm-force-resolutions",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "format": "prettier .  --write"
    },
    "dependencies": {
        "@ag-grid-community/client-side-row-model": "^32.1.0",
        "@ag-grid-community/styles": "^32.1.0",
        "@ag-grid-community/vue3": "^32.1.0",
        "@apollo/client": "3.12.7",
        "@frsource/autoresize-textarea": "^2.0.122",
        "@googlemaps/markerclusterer": "^2.5.3",
        "@microsoft/signalr": "^8.0.7",
        "@toast-ui/editor": "^3.2.2",
        "@vee-validate/i18n": "^4.15.0",
        "@vee-validate/rules": "^4.15.0",
        "@vue/apollo-option": "^4.2.0",
        "autonumeric": "^4.10.5",
        "axios": "^1.8.2",
        "bootstrap": "^5.3.3",
        "bootstrap-select": "^1.13.18",
        "chart.js": "^4.4.7",
        "chartjs-adapter-moment": "^1.0.1",
        "datatables.net": "2.2.1",
        "datatables.net-select": "2.1.0",
        "datatables.net-vue3": "3.0.4",
        "dompurify": "3.2.4",
