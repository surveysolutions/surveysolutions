    "jest-environment-jsdom": "29.0.3",
    "json-schema": ">=0.4.0",
    "json5": "2.2.3",
    "yargs-parser": ">=13.1.2",
    "merge": ">=2.1.1",
    "moment": "2.29.4",
    "loader-utils": "2.0.4",
    "nanoid": ">=2.1.11",
    "node-fetch": "3.2.10",
