  <nlog throwExceptions="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <variable name="logDirectory" value="${basedir}/App_Data/" />
    <targets async="true">
      <target name="logfile" xsi:type="File" fileName="${logDirectory}/logs/${shortdate}.log" layout="${longdate}[${level}] - ${message} [${onexception:${exception:format=tostring}|${stacktrace}}]" />
    </targets>
    <rules>
      <logger name="*" minlevel="Info" writeTo="logfile" />
    </rules>
  </nlog>
  <Exceptional applicationName="WebTester">
    <!-- This entire section is optional, if you want to log all exceptions you can leave off <IgnoreErrors> -->
    <IgnoreErrors>
      <Regexes>
        <!-- Exceptions with messages matching a regular expression here will not be logged -->
        <add name="404" pattern="was not found or does not implement" />
      </Regexes>
      <Types>
        <add name="OperationCanceledException" type="System.OperationCanceledException" />
      </Types>
    </IgnoreErrors>
    <LogFilters>
      <Form>
        <add name="password" replaceWith="***" />
        <add name="confirmPassword" replaceWith="***" />
      </Form>
      <Cookies>
        <add name="__RequestVerificationToken" replaceWith="***" />
        <add name=".AspNet.Cookies" replaceWith="***" />
        <add name="ASP.NET_SessionId" replaceWith="***" />
      </Cookies>
    </LogFilters>
    <!-- JSON: size defaults to 200, this is how many files are kept before the oldest error is removed -->
    <ErrorStore type="JSON" path="~\App_Data\Elmah" size="2000" />
  </Exceptional>
