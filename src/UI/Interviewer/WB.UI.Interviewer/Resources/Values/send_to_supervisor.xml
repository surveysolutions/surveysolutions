<?xml version="1.0" encoding="utf-8" ?>
<resources>
    <style name="SendToSupervisor_Note" parent="AppTheme">
        <item name="android:layout_marginTop">@dimen/SendToSupervisor_Note_MarginTop</item>
        <item name="android:fontFamily">Roboto</item>
        <item name="android:textSize">@dimen/textsize_18</item>
        <item name="android:textColor">@color/offline_sync_regular_text</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="SendToSupervisor_Retry" parent="AppTheme">
        <item name="android:background">@drawable/default_input_button</item>
        <item name="android:textSize">@dimen/textsize_12</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="SendToSupervisor_Navigation" parent="AppTheme">
        <item name="android:layout_marginTop">@dimen/SendToSupervisor_Navigation_MarginTop</item>
    </style>
</resources>
