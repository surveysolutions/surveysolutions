<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" xmlns:local="http://schemas.android.com/apk/res-auto">
  <EditTextPreference android:key="HttpResponseTimeout" android:numeric="integer" android:defaultValue="@integer/HttpResponseTimeout"/>

  <EditTextPreference android:key="GpsReceiveTimeoutSec" android:numeric="integer" android:defaultValue="@integer/GpsReceiveTimeoutSec"/>
  <EditTextPreference android:key="GpsDesiredAccuracy" android:numeric="integer" android:defaultValue="@integer/GpsDesiredAccuracy"/>

  <CheckBoxPreference android:key="VibrateOnError" />
  <CheckBoxPreference android:key="ShowLocationOnMap" />
  
  <CheckBoxPreference android:key="ShowVariables" />
  <Preference android:key="version" />
  
  <PreferenceCategory android:key="dev_settings_category">
    <EditTextPreference android:key="DesignerEndpoint" android:defaultValue="@string/DesignerEndpoint"/>
    <CheckBoxPreference android:key="AcceptUnsignedSslCertificate" android:defaultValue="@bool/AcceptUnsignedSslCertificate" />
  </PreferenceCategory>
  
</PreferenceScreen>