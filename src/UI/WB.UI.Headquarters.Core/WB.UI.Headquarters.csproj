    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="7.0.0" />
    <PackageReference Include="HotChocolate.AspNetCore" Version="11.0.0-preview.120" />
    <PackageReference Include="HotChocolate.AspNetCore.Voyager" Version="10.4.1" />
    <PackageReference Include="HotChocolate.Types.Filters" Version="11.0.0-preview.120" />
    <PackageReference Include="HotChocolate.Types.Selections" Version="11.0.0-preview.120" />
    <PackageReference Include="HotChocolate.Types.Sorting" Version="11.0.0-preview.120" />
    <PackageReference Include="Humanizer.Core.ar" Version="2.7.9" />
    <PackageReference Include="Humanizer.Core.es" Version="2.7.9" />
    <PackageReference Include="Humanizer.Core.fr" Version="2.7.9" />
