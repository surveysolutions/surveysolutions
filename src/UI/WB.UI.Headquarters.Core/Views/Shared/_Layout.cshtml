@using WB.UI.Shared.Web.Extensions
@using System.Globalization
<!DOCTYPE html>
<html lang="@CultureInfo.CurrentUICulture.Name">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Html.GenerateFavicon(Url.Static("/img/identity/favicon-hq"))
    <title>@ViewBag.Title</title>
    <partial name="GlobalSettings" />
    <locale component="hq"></locale>
    @RenderSection("scripts", false)
<link href="/js/chunk-common.js" rel="preload" as="script"><link href="/js/chunk-vendors.js" rel="preload" as="script"><link href="/js/hq_vue.js" rel="preload" as="script"></head>
<body class='wide-navbar'>
    <header>
        <partial name="NavigationBar" />
    </header>
    <partial name="NoticeBar"/>
    @RenderBody()
    <partial name="Footer" />
    <partial name="_Clickjacking"/>
<script type="text/javascript" src="/js/chunk-vendors.js"></script><script type="text/javascript" src="/js/chunk-common.js"></script><script type="text/javascript" src="/js/hq_vue.js"></script></body>
</html>
