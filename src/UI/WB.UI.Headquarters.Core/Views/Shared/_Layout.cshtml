@using WB.UI.Shared.Web.Extensions
@using System.Globalization
<!DOCTYPE html>
<html lang="@CultureInfo.CurrentUICulture.Name">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Html.GenerateFavicon(Url.Static("/img/identity/favicon-hq"))
    <title>@ViewBag.Title</title>
    <partial name="GlobalSettings" />
    <locale component="hq"></locale>
    @RenderSection("scripts", false)
  <script type="module" crossorigin src="/js/markup.js"></script>
  <script type="module" crossorigin src="/js/vendor.js"></script>
  <script type="module" crossorigin src="/js/index.js"></script>
  <script type="module" crossorigin src="/js/compatibility.js"></script>
  <script type="module" crossorigin src="/js/main.js"></script>
  <link rel="stylesheet" crossorigin href="/css/markup.css">
  <link rel="stylesheet" crossorigin href="/css/vendor.css">
  <link rel="stylesheet" crossorigin href="/css/index.css">
  <link rel="stylesheet" crossorigin href="/css/main.css">
  <link rel="stylesheet" crossorigin href="/css/markup-specific.css">
</head>
<body class='wide-navbar'>
    <header>
        <partial name="NavigationBar" />
    </header>
    <partial name="NoticeBar"/>
    @RenderBody()
    <partial name="Footer" />
    <partial name="_Clickjacking"/>
</body>
</html>
