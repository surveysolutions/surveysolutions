<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="Theme.Splash" parent="@android:style/Theme.Light">
    <item name="android:windowNoTitle">true</item>
  </style>

  <style name="dashboardSearchEditText" parent="@android:style/Widget.Holo.Light.EditText">
    <item name="android:textCursorDrawable">@drawable/color_cursor</item>
    <item name="android:textColor">@color/text</item>
    <item name="android:textColorHint">@color/hint</item>
  </style>

  <color name="text">#FFFFFFFF</color>
  <color name="hint">#ffd3d3d3</color>
  <color name="background">#FF2A81CB</color>
  <color name="errorMessage">#FFE32102</color>

  <color name="enabledQuestionnaire">#FF2FC507</color>
  <color name="disabledQuestionnaire">#FFE9E9E9</color>

  <color name="progressIndicatorFirst">#90EE90</color>
  <color name="progressIndicatorSecond">#FFFFFFFF</color>
  <color name="progressIndicatorThird">#EE82EE</color>
  <color name="progressIndicatorFourth">#FFFFFFFF</color>
</resources>