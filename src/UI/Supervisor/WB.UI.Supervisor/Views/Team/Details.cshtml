@using System.Web.Optimization
@model WB.UI.Supervisor.Models.UserViewModel

@{
    ViewBag.Title = string.Format("Edit user: {0}", Model.UserName);
}

@section scripts
{
    @Scripts.Render("~/validate")
}
<div class="page-header">
    <h2>
        Edit user
        <small>
            @Model.UserName
        </small>
    </h2>
</div>


@using (Html.BeginForm())
{
    <fieldset class="form-horizontal">
        @{
            this.Html.RenderPartial("EditForm");
        }
        <div class="form-group">
            <div class="col-sm-offset-2">
                <button type="submit" class="btn btn-primary">Save changes</button>
                @Html.ActionLink("Cancel", "DetailsBack", new { id = Model.Id }, new { @class = "btn btn-default" })
            </div>
        </div>
    </fieldset>
}
