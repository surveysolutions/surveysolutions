<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="Test" value="ank.Headquarters.Web.config" />
    <add key="Raven.DocumentStore" value="http://localhost:8080" />
    <add key="Raven.Databases.Events" value="HeadquartersEvents" />
    <add key="Raven.Databases.Views" value="HeadquartersViews" />
    <add key="SupervisorFunctionsEnabled" value="true"/>
    <add key="Raven.Databases.PlainStorage" value="HeadquartersPlainStorage" />
  </appSettings>
  <system.serviceModel>
    <client>
      <endpoint address="https://localhost/Designer/PublicService.svc"
        binding="wsHttpBinding" bindingConfiguration="CustomBinding_IPublicService"
        contract="PublicService.IPublicService" name="CustomBinding_IPublicService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
    </client>
    <bindings>
      <wsHttpBinding>
        <binding name="CustomBinding_IPublicService" maxReceivedMessageSize="2147483647">
          <readerQuotas maxDepth="2147483647" maxStringContentLength="2147483647"
                       maxArrayLength="2147483647" maxBytesPerRead="2147483647" maxNameTableCharCount="2147483647" />
          <security mode="TransportWithMessageCredential">
            <transport clientCredentialType="None" />
            <message clientCredentialType="UserName" establishSecurityContext="false" />
          </security>
        </binding>
      </wsHttpBinding>
    </bindings>
  </system.serviceModel>
</configuration>
