@model String
@{
    Layout = "~/Views/Shared/Profile/_FormFieldPartialLayout.cshtml";

    var htmlAttributes = new Dictionary<string, object>()
    {
        { "class", @"form-control with-clear-btn" },
        { "data_val", (!ViewData.ContainsKey("disableClientValidation")).ToString().ToLower() },
    };
    var autocomplete = ViewData["autocomplete"];
    if (autocomplete != null)
    {
        htmlAttributes["autocomplete"] = autocomplete;
    }
}
@Html.TextBoxFor(v => v, htmlAttributes)
