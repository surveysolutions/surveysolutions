
<script type="text/html" id="datatable-pager">
@*
    <div class="dataTables_info" role="status" aria-live="polite">
        Showing 1 to 50 of 703 entries
    </div>
*@
    <div class="dataTables_paginate paging_full_numbers" id="supervisors_paginate" style="display: block;">
        <!-- ko if:CurrentPage() === 1 -->
        <a class="paginate_button first disabled" tabindex="0" href="#">@DataTables.Paginate_First</a>
        <!-- /ko -->
        <!-- ko ifnot:CurrentPage() === 1 -->
        <a class="paginate_button first" tabindex="0" href="#" data-bind="click: function () { CurrentPage(1); }">@DataTables.Paginate_First</a>
        <!-- /ko -->
        <!-- ko if:HasPrevPage -->
        <a class="paginate_button previous" tabindex="0" href="#" data-bind="click: function () { CurrentPage(CurrentPage() - 1); }">@DataTables.Paginate_Previous</a>
        <!-- /ko -->
        <!-- ko ifnot:HasPrevPage -->
        <a class="paginate_button previous disabled" tabindex="0" href="#">@DataTables.Paginate_Previous</a>
        <!-- /ko -->
        <span>
            <!-- ko foreach:Pages -->
            <!-- ko if:$data === $parent.CurrentPage() -->
            <a class="paginate_button current" tabindex="0" href="#" data-bind="text: $data"></a>
            <!-- /ko -->
            <!-- ko if:$data !== $parent.CurrentPage() -->
            <a class="paginate_button" tabindex="0" href="#" data-bind="text: $data, click: function () { $parent.CurrentPage($data); }"></a>
            <!-- /ko -->
            <!-- /ko -->
        </span>
        <!-- ko if:HasNextPage -->
        <a class="paginate_button next" tabindex="0" href="#" data-bind="click: function () { CurrentPage(CurrentPage() + 1); }">@DataTables.Paginate_Next</a>
        <!-- /ko -->
        <!-- ko ifnot:HasNextPage -->
        <a class="paginate_button next disabled" tabindex="0" href="#">@DataTables.Paginate_Next</a>
        <!-- /ko -->
        <!-- ko ifnot:CurrentPage() === LastPage() -->
        <a class="paginate_button last" tabindex="0" href="#" data-bind="click: function () { CurrentPage(LastPage()); }">@DataTables.Paginate_Last</a>
        <!-- /ko -->
        <!-- ko if:CurrentPage() === LastPage() -->
        <a class="paginate_button last disabled" tabindex="0" href="#">@DataTables.Paginate_Last</a>
        <!-- /ko -->
    </div>
</script>

