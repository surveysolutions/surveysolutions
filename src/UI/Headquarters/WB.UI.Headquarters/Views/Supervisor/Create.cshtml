@using System.Web.Optimization
@model WB.Core.SharedKernels.SurveyManagement.Web.Models.UserModel

@{
    ViewBag.Title = Users.CreateSupervisor;
    Layout = "~/Views/Shared/_EditorLayout.cshtml";
}

@section scripts
{
    @Scripts.Render("~/validate")
    <script type="text/javascript">
        $('#umbrella').hide();
    </script>
}
@Html.Partial("_alerts")
<div class="page-header">
    <h2>@Users.CreateSupervisor</h2>
</div>



@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, null, new Dictionary<string, object> { { "class", "alert alert-danger" } })
    <fieldset class="form-horizontal">
        @{
            this.Html.RenderPartial("UserCreateForm");
        }
        <div class="form-group">
            <div class="col-sm-offset-2">
                <button type="submit" class="btn btn-primary">@Common.SaveChanges</button>
                @Html.ActionLink(Common.Cancel, "Index", null, new { @class = "btn btn-default" })
            </div>
        </div>
    </fieldset>
}
