@using System.Web.Optimization
@model WB.UI.Headquarters.Models.UserCreateModel

@{
    ViewBag.Title = "Add interviewer";
}

@section scripts
{
    @Scripts.Render("~/validate")
}

<h2>Interviewer <small>create</small></h2>

@using (Html.BeginForm())
{
    <fieldset class="form-horizontal">
        @{
            this.Html.RenderPartial("EditForm");
        }
        <div class="form-actions">
            <button type="submit" class="btn btn-primary">Save changes</button>
            @Html.ActionLink("Cancel", "Interviewers", new { id = Model.Id }, new { @class = "btn " })
        </div>
    </fieldset>
}
<div>
    @Html.ActionLink("Back to List", "Interviewers", new { id = Model.Id })
</div>
