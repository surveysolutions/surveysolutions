@using System.Web.Optimization
@model WB.UI.Headquarters.Models.UserCreateModel

@{
    ViewBag.Title = "Add interviewer";
}

@section scripts
{
    @Scripts.Render("~/validate")
}

<div class="page-header">
    <h2>
        Interviewer <small>create</small>
    </h2>
</div>


<form class="form-horizontal col-sm-6" method="POST">
    @{
        this.Html.RenderPartial("EditForm");
    }
    <div class="form-group">
        <div class="col-sm-offset-4">
            <button type="submit" class="btn btn-primary">Save changes</button>
            @Html.ActionLink("Cancel", "Interviewers", new { id = Model.Id }, new { @class = "btn btn-default" })
        </div>
    </div>
</form>
