@using WB.Enumerator.Native.Resources
@model WB.Enumerator.Native.WebInterview.Models.FinishWebInterview
@{
    Layout = "_WebInterviewLayout.cshtml";
}
<div class="centered-box-table">
	<h2>@WebInterview.WebSurvey</h2>
	<div class="additional-info-block">
		<ul class="list-unstyled">
			<li><strong>@WebInterview.FinishInterviewText</strong></li>
			<li><strong>@string.Format(WebInterview.SurveyFormatText, Model.QuestionnaireTitle)</strong></li>
		</ul>
	</div>
	<div class="additional-info-block">
		<ul class="list-unstyled">
			@if (Model.StartedDate.HasValue)
			{
			<li>@WebInterview.InterviewStartDateFormatText <time datetime="@Model.StartedDate.Value.ToString(" o")"></time></li>
			}
			@if (Model.CompletedDate.HasValue)
			{
			<li>@WebInterview.InterviewCompletionDateFormatText <time datetime="@Model.CompletedDate.Value.ToString(" o")"></time></li>
			}
		</ul>
	</div>
</div>