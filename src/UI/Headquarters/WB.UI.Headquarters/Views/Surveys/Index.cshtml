@model WB.Core.BoundedContexts.Headquarters.Views.Survey.SurveyLineView[]

@{
    ViewBag.Title = "Surveys";
}


<div>
    <h1>@ViewBag.Title</h1>
    <div class="row col-sm-12 bottom-buffer">
        @Html.ActionLink("Start new survey", "StartNew", "Surveys", null, new { @class = "btn btn-default" })
    </div>
    <div class="row col-sm-12">
        <table class="table table-striped">
            <tr>
                <th>Name</th>
            </tr>
            @foreach (var survey in Model)
            {
                <tr>
                    <td>@survey.Name</td>
                </tr>
            }
        </table>
    </div>
</div>