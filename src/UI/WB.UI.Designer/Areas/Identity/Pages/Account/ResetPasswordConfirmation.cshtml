@page
@model ResetPasswordConfirmationModel
@{
    ViewBag.Title = AccountResources.PasswordReset;
}

<form>
    <h1>
       @AccountResources.ThankYou
    </h1>
    <p class="note">
        @AccountResources.PasswordSet
    </p>
    <div class="form-actions">
        <a class="btn btn-success btn-lg" asp-page="./Login">@AccountResources.BackToLogin</a>
    </div>
</form>

