
// do not edit this file with Notepad.exe

clear all
use "StataWriter14_T26.dta", clear

assert c(N)==5
assert c(k)==2

// check variable names
quietly ds
assert r(varlist)=="x y"

// check variable types
confirm double variable x
confirm long variable y

// check variable formats
assert "`:format x'"=="%6.2f"
assert "`:format y'"=="%6.0f"

// check the values are missing where appropriate
assert x[1]==1
assert x[2]==0.00005
assert x[3]==.a
assert x[4]==3.14159265
assert x[5]==1.0e-22

assert y[1]==2
assert y[2]==.a
assert y[3]==912300
assert y[4]==123450
assert y[5]==100000


// test is now completed successfully, create the marker
display filewrite("StataWriter14_T26.txt","ok")

exit, STATA clear
